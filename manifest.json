{
    "name": "TrainingSpace Buddy",
    "description" : "Расширение для тренеров TrainingSpace.",
    "version": "0.1",
    "manifest_version": 2,
    "permissions": [
      "desktopCapture",
      "storage",
    	"tabs",
      "<all_urls>",
      "activeTab" ],
    "content_scripts" : [{
      "matches" : [
        "http://127.0.0.1:8080/*",
        "http://localhost:8080/*",
        "http://localhost:4444/app/room/*",
        "http://127.0.0.1:4444/app/room/*",
        "https://trainingspace.oblakogroup.ru/app/room/*",
        "https://trainingspace.online/app/room/*"
      ],
      "js" : ["inject.js"]
    }],
    "background": {
      "scripts": ["background.js"]
    },
    "web_accessible_resources": ["script.js"]
}